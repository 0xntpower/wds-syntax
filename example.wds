$$ POP R32, POP R32, RET gadget search
$$ Modified to avoid POP ESP (0x5C)
$$ Usage: $$>a< script.wds start_addr end_addr
.block
{
        .for (r $t0 = 0x58; $t0 < 0x5F; r $t0 = $t0 + 0x01)
        {
		    .for (r $t1 = 0x58; $t1 < 0x5F; r $t1 = $t1 + 0x01)
		    {
                        .if (@@C++((@$t0 != 0x5C) && (@$t1 != 0x5C)))
                        {
                                s-[1]b ${$arg1} ${$arg2} $t0 $t1 0xC3
                        }
		    }
        }
}
